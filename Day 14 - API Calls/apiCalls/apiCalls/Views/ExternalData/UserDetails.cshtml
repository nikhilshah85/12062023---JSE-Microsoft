


<script>


    function getUserDetails()
    {
        var userDataUrl = "https://jsonplaceholder.typicode.com/users";

        fetch(userDataUrl).then(format => format.json())
                          .then(userData => {
                              console.log(userData);

                              for(var i=0;i<userData.length;i++)
                              {
                                  $("table").append("<tr>" +
                                    "<td>" + userData[i].id + "</td>" + 
                                    "<td>" + userData[i].name + "</td>" + 
                                    "<td>" + userData[i].username + "</td>" + 
                                    "<td>" + userData[i].email + "</td>" +
                        "<td>" + userData[i].address.city + "</td>" +
                        "<td>" + userData[i].address.geo.lat + "</td>" +
                        "<td>" + userData[i].company.name + "</td>" +
                        "<td>" + userData[i].company.catchPhrase + "</td>" +

                                  
                                  + "</tr>")
                              }
                          })
                          .catch(errDetails =>{
                              console.log(errDetails);
                          })
    }
</script>


<table class="table table-stripped"></table>
<button class="btn btn-warning" onclick="getUserDetails()"> Display Users</button>